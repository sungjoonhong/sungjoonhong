<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.sungjoonhong.springboot.dao.MemberDAO">
        
    <select id="selectMember" resultType="com.sungjoonhong.springboot.domain.MemberVO">
        SELECT * FROM sungjoonhong.member
    </select>
    
    <insert id="insertMember">
        INSERT INTO sungjoonhong.member(name) VALUES(#{name})
    </insert>
    
</mapper>